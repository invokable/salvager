{
  "name": "revolution/salvager",
  "description": "Tiny WebCrawler for Laravel using Playwright",
  "keywords": [
    "laravel",
    "playwright"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.4",
    "victor-teles/playwright-php": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.4",
    "mockery/mockery": "^1.6.1",
    "orchestra/testbench": "^10.8",
    "laravel/pint": "^1.26",
    "revolution/laravel-boost-copilot-cli": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Revolution\\Salvager\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "kawax",
      "email": "kawaxbiz@gmail.com"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Revolution\\Salvager\\Providers\\SalvagerServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "lint": "pint"
  }
}
